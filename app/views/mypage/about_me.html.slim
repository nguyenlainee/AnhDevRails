                p.text-muted.mb-0
                  = 'Nguyen Hoang Lai'
            .row.mt-2
              .col-sm-3
                p.mb-0
                  = t '.email'
              .col-sm-9
                p.text-muted.mb-0
                  = 'nguyenlai1706@gmail.com'
            .row.mt-2
              .col-sm-3
                p.mb-0
                  = t '.phone'
              .col-sm-9
                p.text-muted.mb-0
