      .col-lg-4
        .card.mb-4
          .card-body.text-center
            img.rounded-circle.img-fluid src='https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-chat/ava3.webp' alt='Avatar' style='width: 150px;'
            h5.my-3
              = 'Hoang Lai'
            p.text-muted.mb-1
              = 'Full Stack Developer'
            p.text-muted.mb-4
              = 'Go Vap, Ho Chi Minh City'
